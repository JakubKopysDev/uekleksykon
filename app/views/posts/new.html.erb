<div class="content_other">
  <div class="new_post">

    <%= form_for @post do |f| %>

      <div class="field">
        <%= f.label :title %><br>
        <%= f.text_field :title %>
      </div>

      <div class="field">
        <%= f.label :content %><br>
        <%= f.text_area :content %>
      </div>

      <%= f.submit %>

    <% end %>

  </div>
</div>
